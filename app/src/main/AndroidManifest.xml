<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.flacoapps.workoutassistant2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".TopLevelActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ShouldersCategoryActivity" />
        <activity android:name=".BackCategoryActivity" />
        <activity android:name=".ChestCategoryActivity" />
        <activity android:name=".BicepsCategoryActivity" />
        <activity android:name=".TricepsCategoryActivity" />
        <activity android:name=".ForearmsCategoryActivity" />
        <activity android:name=".AbsCategoryActivity" />
        <activity android:name=".LegsCategoryActivity" />
        <activity android:name=".AbsDetailActivity" />
        <activity android:name=".BackDetailActivity" />
        <activity android:name=".BicepsDetailActivity"></activity>
    </application>

</manifest>